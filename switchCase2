package chapter2;

public class SwitchCase2 {

	public static void main(String[] args) {
		String medal = "Gold";
		
		switch(medal) {
		case "Gold" :
			System.out.println("금메달입니다.");
			break;
		case "Bronze" :
			System.out.println("브론즈입니다.");
			break;
		case "Silver" :
			System.out.println("실버입니다.");
		default :
			System.out.println("메달이 없습니다.");
			break;
		}
	}
}
